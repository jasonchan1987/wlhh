<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bootdo.su.dao.StatehisDao">

	<select id="get" resultType="com.bootdo.su.domain.StatehisDO">
		select `shid`,`srid`,`state`,`reason`,`create_by`,`create_deptid`,`create_date`,`update_by`,`update_deptid`,`update_date`,`remarks`,`del_flag` from su_statehis where shid = #{value}
	</select>

	<select id="list" resultType="com.bootdo.su.domain.StatehisDO">
		select `shid`,`srid`,`state`,`reason`,`create_by`,`create_deptid`,`create_date`,`update_by`,`update_deptid`,`update_date`,`remarks`,`del_flag` from su_statehis
        <where>  
		  		  <if test="shid != null and shid != ''"> and shid = #{shid} </if>
		  		  <if test="srid != null and srid != ''"> and srid = #{srid} </if>
		  		  <if test="state != null and state != ''"> and state = #{state} </if>
		  		  <if test="reason != null and reason != ''"> and reason = #{reason} </if>
		  		  <if test="createBy != null and createBy != ''"> and create_by = #{createBy} </if>
		  		  <if test="createDeptid != null and createDeptid != ''"> and create_deptid = #{createDeptid} </if>
		  		  <if test="createDate != null and createDate != ''"> and create_date = #{createDate} </if>
		  		  <if test="updateBy != null and updateBy != ''"> and update_by = #{updateBy} </if>
		  		  <if test="updateDeptid != null and updateDeptid != ''"> and update_deptid = #{updateDeptid} </if>
		  		  <if test="updateDate != null and updateDate != ''"> and update_date = #{updateDate} </if>
		  		  <if test="remarks != null and remarks != ''"> and remarks = #{remarks} </if>
		  		  <if test="delFlag != null and delFlag != ''"> and del_flag = #{delFlag} </if>
		  		</where>
        <choose>
            <when test="sort != null and sort.trim() != ''">
                order by ${sort} ${order}
            </when>
			<otherwise>
                order by shid desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="count" resultType="int">
		select count(*) from su_statehis
		 <where>  
		  		  <if test="shid != null and shid != ''"> and shid = #{shid} </if>
		  		  <if test="srid != null and srid != ''"> and srid = #{srid} </if>
		  		  <if test="state != null and state != ''"> and state = #{state} </if>
		  		  <if test="reason != null and reason != ''"> and reason = #{reason} </if>
		  		  <if test="createBy != null and createBy != ''"> and create_by = #{createBy} </if>
		  		  <if test="createDeptid != null and createDeptid != ''"> and create_deptid = #{createDeptid} </if>
		  		  <if test="createDate != null and createDate != ''"> and create_date = #{createDate} </if>
		  		  <if test="updateBy != null and updateBy != ''"> and update_by = #{updateBy} </if>
		  		  <if test="updateDeptid != null and updateDeptid != ''"> and update_deptid = #{updateDeptid} </if>
		  		  <if test="updateDate != null and updateDate != ''"> and update_date = #{updateDate} </if>
		  		  <if test="remarks != null and remarks != ''"> and remarks = #{remarks} </if>
		  		  <if test="delFlag != null and delFlag != ''"> and del_flag = #{delFlag} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.bootdo.su.domain.StatehisDO" useGeneratedKeys="true" keyProperty="shid">
		insert into su_statehis
		(
			`srid`, 
			`state`, 
			`reason`, 
			`create_by`, 
			`create_deptid`, 
			`create_date`, 
			`update_by`, 
			`update_deptid`, 
			`update_date`, 
			`remarks`, 
			`del_flag`
		)
		values
		(
			#{srid}, 
			#{state}, 
			#{reason}, 
			#{createBy}, 
			#{createDeptid}, 
			#{createDate}, 
			#{updateBy}, 
			#{updateDeptid}, 
			#{updateDate}, 
			#{remarks}, 
			#{delFlag}
		)
	</insert>
	 
	<update id="update" parameterType="com.bootdo.su.domain.StatehisDO">
		update su_statehis 
		<set>
			<if test="srid != null">`srid` = #{srid}, </if>
			<if test="state != null">`state` = #{state}, </if>
			<if test="reason != null">`reason` = #{reason}, </if>
			<if test="createBy != null">`create_by` = #{createBy}, </if>
			<if test="createDeptid != null">`create_deptid` = #{createDeptid}, </if>
			<if test="createDate != null">`create_date` = #{createDate}, </if>
			<if test="updateBy != null">`update_by` = #{updateBy}, </if>
			<if test="updateDeptid != null">`update_deptid` = #{updateDeptid}, </if>
			<if test="updateDate != null">`update_date` = #{updateDate}, </if>
			<if test="remarks != null">`remarks` = #{remarks}, </if>
			<if test="delFlag != null">`del_flag` = #{delFlag}</if>
		</set>
		where shid = #{shid}
	</update>
	
	<delete id="remove">
		delete from su_statehis where shid = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from su_statehis where shid in 
		<foreach item="shid" collection="array" open="(" separator="," close=")">
			#{shid}
		</foreach>
	</delete>

</mapper>